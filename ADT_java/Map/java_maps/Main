import java.util.*;

public class Main {
	public static void main(String args[]) {
		String[] data = "banana apple donkey banana caramel elephant".split(" ");

		HashMap<String, Integer> m = new HashMap<>();
		LinkedHashMap<String, Integer> lhm = new LinkedHashMap<>();
		TreeMap<String, Integer> tm = new TreeMap<>();

		for(int i=0;i<data.length;i++){
			String key = data[i];

			// HashMap
			if(m.get(key) == null) m.put(key, 1);
			else                   m.put(key,m.get(key)+1);

			// LinkedHashMap
			if(lhm.get(key) == null) lhm.put(key, 1);
			else                     lhm.put(key,lhm.get(key)+1);

			// TreeMap
			if(tm.get(key) == null) tm.put(key, 1);
			else                    tm.put(key,tm.get(key)+1);
		}

		System.out.println("Hash map: " + m);
		System.out.println("LinkedHash map: " + lhm);
		System.out.println("Tree map: " + tm);
		System.out.println();

		// not ordered
		for(Map.Entry<String, Integer> entry: m.entrySet()) //entrySet() returns an iterable collection
			System.out.print(entry+" ");
		System.out.println();

		// retrieved in insertion order
		for(Map.Entry<String, Integer> entry: lhm.entrySet()) //entrySet() returns an iterable collection
			System.out.print(entry+" ");
		System.out.println();

		// sorted
		for(Map.Entry<String, Integer> entry: tm.entrySet()) //entrySet() returns an iterable collection
			System.out.print(entry+" ");
	}
}